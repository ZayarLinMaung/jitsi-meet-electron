{
  "name": "medcom-electron",
  "version": "2025.4.0",
  "description": "Electron application for Medcom",
  "main": "./build/main.js",
  "productName": "Medcom",
  "private": true,
  "scripts": {
    "start": "webpack --config ./webpack.main.js --mode development && concurrently \"npm:watch\" \"electron ./build/main.js --no-sandbox\"",
    "clean": "rm -rf node_modules build dist",
    "lint": "eslint . && flow",
    "build": "webpack --config ./webpack.main.js --mode production && webpack --config ./webpack.renderer.js --mode production",
    "dist": "npm run build && electron-builder",
    "postinstall": "patch-package",
    "watch": "webpack --config ./webpack.renderer.js --mode development --watch --watch-options-poll=1000"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "build": {
    "appId": "com.medcom.app",
    "productName": "Medcom",
    "generateUpdatesFilesForAllChannels": true,
    "npmRebuild": false,
    "files": [
      "build",
      "resources",
      "!app",
      "!main.js"
    ],
    "mac": {
      "artifactName": "jitsi-meet.${ext}",
      "target": [
        {
          "arch": "universal",
          "target": "dmg"
        },
        {
          "arch": "universal",
          "target": "zip"
        }
      ],
      "category": "public.app-category.video",
      "darkModeSupport": true,
      "entitlements": "resources/entitlements.mac.plist",
      "extendInfo": {
        "NSCameraUsageDescription": "Medcom requires access to your camera in order to make video-calls.",
        "NSMicrophoneUsageDescription": "Medcom requires access to your microphone in order to make calls (audio/video).",
        "LSMultipleInstancesProhibited": true
      }
    },
    "mas": {
      "entitlements": "resources/entitlements.mas.plist",
      "entitlementsInherit": "resources/entitlements.mas.inherit.plist",
      "hardenedRuntime": false
    },
    "linux": {
      "artifactName": "medcomvision-${arch}.${ext}",
      "category": "VideoConference;AudioVideo;Audio;Video;Network",
      "description": "MedcomVision Desktop App",
      "executableName": "medcomvision",
      "target": [
        {
          "arch": "x64",
          "target": "AppImage"
        },
        {
          "arch": "x64",
          "target": "deb"
        },
        {
          "arch": "arm64",
          "target": "AppImage"
        },
        {
          "arch": "arm64",
          "target": "deb"
        }
      ]
    },
    "win": {
      "artifactName": "medcomvision.${ext}",
      "target": [
        {
          "arch": [
            "ia32",
            "x64",
            "arm64"
          ],
          "target": "nsis"
        }
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "protocols": [
      {
        "name": "medcomvision-protocol",
        "role": "Viewer",
        "schemes": [
          "medcomvision"
        ]
      }
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jitsi/jitsi-meet-electron"
  },
  "keywords": [
    "webrtc",
    "electron",
    "medcomvision",
    "video-conference"
  ],
  "author": "MedcomVision Team <contact@medcomvision.com>",
  "readmeFilename": "README.md",
  "license": "Apache-2.0",
  "dependencies": {
    "@jitsi/electron-sdk": "7.0.6",
    "electron-debug": "^3.2.0",
    "electron-reload": "^1.5.0",
    "jsqr": "^1.4.0"
  },
  "devDependencies": {
    "@atlaskit/button": "^10.1.3",
    "@atlaskit/css-reset": "^3.0.8",
    "@atlaskit/droplist": "^7.0.19",
    "@atlaskit/field-text": "^7.1.0",
    "@atlaskit/icon": "^15.0.3",
    "@atlaskit/navigation": "^33.3.10",
    "@atlaskit/onboarding": "^6.2.0",
    "@atlaskit/page": "^8.0.12",
    "@atlaskit/spinner": "^9.0.13",
    "@atlaskit/theme": "^7.0.5",
    "@atlaskit/toggle": "^5.0.15",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-transform-flow-strip-types": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@hapi/bourne": "^3.0.0",
    "@jitsi/js-utils": "2.0.5",
    "@svgr/webpack": "^6.2.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.2.3",
    "concurrently": "5.1.0",
    "css-loader": "^6.7.1",
    "electron": "35.1.5",
    "electron-builder": "26.0.12",
    "electron-context-menu": "^2.5.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.2",
    "electron-react-devtools": "0.5.3",
    "electron-updater": "6.6.2",
    "electron-window-state": "^5.0.3",
    "eslint": "6.5.1",
    "eslint-config-jitsi": "github:jitsi/eslint-config-jitsi#1.0.2",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "22.1.0",
    "eslint-plugin-react": "7.19.0",
    "file-loader": "^6.2.0",
    "flow-bin": "0.109.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.0",
    "i18next": "^19.9.2",
    "moment": "^2.29.2",
    "mousetrap": "^1.6.5",
    "patch-package": "6.5.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.8.12",
    "react-redux": "^5.1.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^3.3.1",
    "styled-components": "^3.4.10",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.9.2"
  }
}
